{
    "name": "Squirrel",
    "version": "0.0.1",
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 6
        },
        "env": {
            "browser": true,
            "jquery": true
        },
        "ecmaFeatures": {
            "blockBindings": true
        },
        "globals": {
        },
        "rules": {
            "strict": [ "error", "global" ],
            "no-underscore-dangle": 0,
            "curly": 0,
            "camelcase": 0,
            "no-unused-vars": 1,
            "no-console": 0,
            "no-debugger": 0,
            "no-alert": 1,
            "no-new" : 0,
            "new-cap" : 0,
            "dot-notation" : 1,
            "no-trailing-spaces" : 0,
            "strict" : 1,
            "no-use-before-define": 1,
            "no-loop-func": 1,
            "no-eval": 2,
            "quotes": 0
        }
    }
}
