{
  "name": "Squirrel",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/cdot/Squirrel",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdot/Squirrel.git"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "shared-node-browser": true,
      "jquery": true
    },
    "globals": {
      "define": false,
      "ArrayBuffer": false,
      "Buffer": false,
      "Uint8Array": false,
      "Uint8ClampedArray": false,
      "Int8Array": false,
      "Int16Array": false,
      "Uint16Array": false,
      "Int32Array": false,
      "Uint32Array": false,
      "Float32Array": false,
      "Float64Array": false,
      "XMLHttpRequest": false,
      "WeakMap": false,
      "require": false,
      "module": false,
      "Promise": false,
      "requirejs": true
    },
    "rules": {
      "strict": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "camelcase": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "no-debugger": 0,
      "no-alert": 1,
      "no-new": 0,
      "new-cap": 0,
      "dot-notation": 1,
      "no-trailing-spaces": 0,
      "no-useless-escape": 1,
      "no-use-before-define": 1,
      "no-loop-func": 1,
      "no-eval": 1,
      "quotes": 0
    }
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.0",
    "requirejs": "^2.3.6"
  },
  "devDependencies": {
    "axios": ">=0.17.1",
    "beautifyjs": "0.0.1",
    "btoa": "^1.2.1",
    "chai": "^4.2.0",
    "chai-http": "*",
    "clean-css": ">=4.1.9",
    "clean-css-cli": ">=4.1.10",
    "cookies": "^0.7.3",
    "eslint": ">=4.13.1",
    "eslint-plugin-promise": "^4.0.1",
    "express": "^4.16.4",
    "express-basic-auth": "^1.1.6",
    "fs-extra": "^7.0.1",
    "html-minifier": "^3.5.21",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.0.2",
    "node-getopt": "^2.2.4",
    "npm-check": "^5.5.2",
    "request": "^2.88.0",
    "uglify-es": "^3.3.9",
    "url-parse": "^1.4.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "xhr2": "^0.1.4"
  }
}
