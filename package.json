{
  "name": "Squirrel",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/cdot/Squirrel",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdot/Squirrel.git"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "shared-node-browser": true,
      "jquery": true
    },
    "globals": {
		"define": false,
		"ArrayBuffer": false,
		"Buffer": false,
		"Uint8Array": false,
		"Uint8ClampedArray": false,
		"Int8Array": false,
		"Int16Array": false,
		"Uint16Array": false,
		"Int32Array": false,
		"Uint32Array": false,
		"Float32Array": false,
		"Float64Array": false,
		"XMLHttpRequest": true,
		"WeakMap": false,
		"require": false,
		"module": false,
		"Promise": false,
		"requirejs": true,
		"localStorage": true
    },
    "rules": {
			"camelcase": 0,
			"curly": 0,
			"dot-notation": 1,
			"new-cap": 0,
			"no-alert": 0,
			"no-console": 0,
			"no-debugger": 0,
			"no-eval": 0,
			"no-fallthrough": 0,
			"no-loop-func": 1,
			"no-mixed-spaces-and-tabs": 0,
			"no-new": 0,
			"no-redeclare": ["error", { "builtinGlobals": true }],
			"no-trailing-spaces": 0,
			"no-underscore-dangle": 0,
			"no-unused-vars": 0,
			"no-useless-escape": 1,
			"no-use-before-define": 0,
			"quotes": 0,
			"strict": 0
    }
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.0",
    "js-cookie": "^3.0.1",
    "mime-types": "^2.1.33",
    "readline-sync": "^1.4.10",
    "requirejs": "^2.3.6",
    "sync-request": "^6.1.0"
  },
  "devDependencies": {
    "axios": "^0.24.0",
    "btoa": "^1.2.1",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "chai-http": "*",
    "clean-css": ">=5.2.2",
    "cookies": "^0.8.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fs-extra": "^10.0.0",
    "html-minifier": "^4.0.0",
    "jsdoc": "^3.6.7",
    "jsdom": "^18.0.1",
    "mocha": "^9.1.3",
    "node-getopt": "^0.3.2",
    "npm-check": "^5.9.2",
    "request": "^2.88.0",
    "uglify-es": "^3.3.9",
    "url-parse": "^1.5.3",
    "xhr2": "^0.2.1"
  }
}
