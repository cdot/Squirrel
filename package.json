{
    "name": "@cdot/squirrel",
    "version": "0.1.0",
    "license": "MIT",
    "type": "module",
    "author": {
        "name": "Crawford Currie",
        "url": "https://github.com/cdot"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cdot/Squirrel.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "dependencies": {
        "@rwap/jquery-ui-touch-punch": "latest",
        "banana-i18n": "^2.3.3",
        "clipboard": "^2.0.11",
        "jquery": "^3.6.4",
        "jquery-ui-dist": "^1.13.2",
        "jquery.cookie": "^1.4.1",
        "ui-contextmenu": "^1.18.1"
    },
    "optionalDependencies": {
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "btoa": "^1.2.1",
        "canvas": "^2.11.0",
        "cors": "^2.8.5",
        "express": "^4.21.0",
        "express-basic-auth": "^1.2.1",
        "express-session": "^1.17.3",
        "node-getopt": "^0.3.2",
        "pngjs": "^7.0.0",
        "posix-getopt": "^1.2.1",
        "session-file-store": "^1.5.0",
        "url": "^0.11.0",
        "xhr2": "^0.2.1"
    },
    "devDependencies": {
        "c8": "^7.13.0",
        "chai": "^4.3.7",
        "chai-http": "*",
        "eslint": "^8.37.0",
        "jsdoc": "^4.0.2",
        "jsdom": "^21.1.1",
        "mocha": "^10.2.0",
        "node-localstorage": "^2.2.1",
        "tmp-promise": "^3.0.3",
        "webdav-server": "^2.6.2",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.0.1"
    },
    "scripts": {
        "build": "webpack -c build/webpack_config.js",
        "doc": "jsdoc --verbose -c jsdoc_config.json -r -p -d doc src",
        "lint": "eslint src -f unix",
        "test": "mocha --timeout=5000 test/*/*.js",
        "coverage": "NODE_ENV=test c8 --reporter=html --reporter text npm run test",
        "tx": "node build/checkStrings.js",
        "server": "node bin/web_server.js --user=x --pass=x --debug --log"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2022
        },
        "env": {
            "shared-node-browser": true,
            "jquery": true,
            "es2021": true
        },
        "globals": {
            "define": false,
            "ArrayBuffer": false,
            "Buffer": false,
            "Uint8Array": false,
            "Uint8ClampedArray": false,
            "Int8Array": false,
            "Int16Array": false,
            "Uint16Array": false,
            "Int32Array": false,
            "Uint32Array": false,
            "Float32Array": false,
            "Float64Array": false,
            "XMLHttpRequest": true,
            "WeakMap": false,
            "require": false,
            "module": false,
            "Promise": false,
            "requirejs": true,
            "localStorage": true
        },
        "rules": {
            "camelcase": 0,
            "curly": 0,
            "dot-notation": 1,
            "new-cap": 0,
            "no-alert": 0,
            "no-console": 0,
            "no-debugger": 0,
            "no-eval": 0,
            "no-fallthrough": 0,
            "no-loop-func": 1,
            "no-mixed-spaces-and-tabs": 0,
            "no-new": 0,
            "no-redeclare": [
                "error",
                {
                    "builtinGlobals": true
                }
            ],
            "no-trailing-spaces": 0,
            "no-underscore-dangle": 0,
            "no-unused-vars": 0,
            "no-useless-escape": 1,
            "no-use-before-define": 0,
            "quotes": 0,
            "strict": 0
        }
    }
}
